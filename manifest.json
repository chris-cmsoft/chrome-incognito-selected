  {
    "name": "Incognito Selected",
    "version": "1.0",
    "description": "Move all selected tabs into incognito tab.",
    "manifest_version": 2,
    "permissions": ["declarativeContent", "tabs"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html"
    }
  }
